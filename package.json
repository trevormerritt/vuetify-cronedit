{
  "private": false,
  "name": "vuetify-cronedit",
  "version": "0.0.2",
  "description": "Crontab string editor for Vuetify",
  "main": "./dist/vuetify-cronedit.umd.js",
  "scripts": {
    "dev": "vue serve SimpleEditor",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/my-component.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/my-component.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/my-component.min.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trevormerritt/vuetify-cronedit.git"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "vuetify",
    "crontab"
  ],
  "author": "Trevor Merritt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trevormerritt/vuetify-cronedit/issues"
  },
  "homepage": "https://github.com/trevormerritt/vuetify-cronedit#readme",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@vue/compiler-sfc": "^3.0.0-beta.7",
    "bili": "^4.9.4",
    "rollup": "^1.17.0",
    "rollup-plugin-vue": "^6.0.0-alpha.7",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {}
}
